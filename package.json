{
  "name": "mfo",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "start": "vite --mode development",
    "build:prod": "tsc && vite build",
    "build:staging": "tsc && vite build --mode staging",
    "preview": "vite preview",
    "format": "prettier . --write",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:staged": "lint-staged --config .lintstagedrc.cjs",
    "prepare": "husky install",
    "codegen": "rimraf src/apis/generate && tsc --esModuleInterop ./generate-api.ts && node ./generate-api.js && rimraf ./generate-api.js"
  },
  "dependencies": {
    "@ant-design/icons": "^5.1.4",
    "@tanstack/react-query": "^4.29.18",
    "@tanstack/react-query-devtools": "^4.29.18",
    "antd": "^5.6.3",
    "axios": "^1.4.0",
    "i18next": "^23.2.3",
    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-locize-backend": "^6.2.2",
    "i18next-resources-to-backend": "^1.1.4",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.0.1",
    "react-router-dom": "^6.14.0",
    "styled-components": "^6.0.0-rc.6"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.6.0",
    "@tanstack/eslint-plugin-query": "^4.29.9",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/node": "^20.3.2",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.2",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "swagger-typescript-api": "^12.0.4",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.2",
    "vite": "^4.3.9"
  }
}
